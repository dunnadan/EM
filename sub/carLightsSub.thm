<?xml version="1.0"?>
<alloy>

<view>

<defaultnode/>

<defaultedge/>

<node>
   <type name="Int"/>
   <type name="LeftTurnSignal"/>
   <type name="Lights"/>
   <type name="RightTurnSignal"/>
   <type name="String"/>
   <type name="univ"/>
   <type name="seq/Int"/>
</node>

<node color="Blue">
   <type name="LowBeamHeadlights"/>
</node>

<node color="Gray">
   <type name="TurnSignal"/>
</node>

<node color="Green">
   <type name="CorneringLights"/>
</node>

<node color="Red">
   <type name="EmergencyBreakLight"/>
</node>

<node color="Yellow">
   <type name="AdaptiveHighBeam"/>
</node>

<node shape="Ellipse">
   <set name="this/AdaptiveHighBeamOn" type="AdaptiveHighBeam"/>
   <set name="this/CorneringLightsOn" type="CorneringLights"/>
   <set name="this/EmergencyBreakLightOn" type="EmergencyBreakLight"/>
   <set name="this/LeftTurnSignalOn" type="LeftTurnSignal"/>
   <set name="this/LowBeamHeadlightsOn" type="LowBeamHeadlights"/>
   <set name="this/RightTurnSignalOn" type="RightTurnSignal"/>
</node>

</view>

</alloy>
